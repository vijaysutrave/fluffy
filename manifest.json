{
	"manifest_version": 2,
	"name": "Fluffy",
	"short_name": "Fluffy",
	"version": "1.0.0",
	"description": "Fluffy - Simple, clean and easy note taking Chrome extension",
	"chrome_url_overrides": {
		"newtab": "public/js/fluffy.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"permissions": [
		"storage"
	]
}