{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "removeComments": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmitOnError": false,
    "module": "commonjs",
  },
  "types": ["jest", "react","react-router","history", "enzyme", "css-color-names", "jest-styled-components", "styled-components", "linkify-it", "url-search-params"],
  "exclude": [
    "node_modules"
  ]
}